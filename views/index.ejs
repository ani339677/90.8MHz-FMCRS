        <div class="member-form">
            <hr id="footer-above-member-form">
            <p>Become a member</p>
            <form action="/" method="post">
                <input type="email" name="email_id" id="em_id" placeholder="Email Id" required>
                <input type="number" name="phone_number" id="ph_no" placeholder="Phone Number" required>

                <% if (if_success === "true"){ %>
                <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
                    <symbol id="check-circle-fill" fill="currentColor" viewBox="0 0 16 16">
                      <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
                    </symbol>
                </svg>
                <div class="alert alert-success alert-dismissible fade show d-flex align-items-center" style="width: 90%;" role="alert">
                    <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Success:"><use xlink:href="#check-circle-fill"/></svg>
                      You have subscribed succesfully! Check your mail inbox.
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                </div>
                <script>
                    document.addEventListener("readystatechange",function(){
                        if (document.readyState=="complete"){
                            swal("Well Done!", "You have subscribed successfully!\nPlease check your mail inbox.", "success");
                        }
                    });
                    
                </script>
                <% } else if (if_success === "false"){ %>
                <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
                    <symbol id="exclamation-triangle-fill" fill="currentColor" viewBox="0 0 16 16">
                        <path d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z"/>
                      </symbol>                    
                </svg>
                <div class="alert alert-danger d-flex align-items-center alert-dismissible fade show" style="width: 90%;" role="alert">
                    <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Danger:"><use xlink:href="#exclamation-triangle-fill"/></svg>
                    <div>
                      Sorry, an error occured while processing. Please try again.
                    </div>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                </div>
                <script>
                    document.addEventListener("readystatechange",function(){
                        if (document.readyState=="complete"){
                            swal("Sorry!", "You couldn't subscribe to us!\nPlease try again.", "error");
                        }
                    });
                    
                </script>
                <% } else if (if_success === "already registered"){ %>
                <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
                    <symbol id="info-fill" fill="currentColor" viewBox="0 0 16 16">
                        <path d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm.93-9.412-1 4.705c-.07.34.029.533.304.533.194 0 .487-.07.686-.246l-.088.416c-.287.346-.92.598-1.465.598-.703 0-1.002-.422-.808-1.319l.738-3.468c.064-.293.006-.399-.287-.47l-.451-.081.082-.381 2.29-.287zM8 5.5a1 1 0 1 1 0-2 1 1 0 0 1 0 2z"/>
                    </symbol>                    
                </svg>
                <div class="alert alert-primary d-flex align-items-center alert-dismissible fade show" style="width: 90%;" role="alert">
                    <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Info:"><use xlink:href="#info-fill"/></svg>
                    <div>
                      Hey! You have already subscribed.
                    </div>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                </div>
                <script>
                    document.addEventListener("readystatechange",function(){
                        if (document.readyState=="complete"){
                            swal("Hey!", "You have already subscribed!\nDon't worry you will be recieving updates ðŸ˜Š", "info");
                        }
                    });
                    
                </script>
                <% } else if (if_success === "invalid data"){ %>
                <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
                    <symbol id="exclamation-triangle-fill" fill="currentColor" viewBox="0 0 16 16">
                        <path d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z"/>
                    </symbol>                    
                </svg>
                <div class="alert alert-warning d-flex align-items-center alert-dismissible fade show" style="width: 90%;" role="alert">
                    <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Warning:"><use xlink:href="#exclamation-triangle-fill"/></svg>
                    <div>
                      Invalid data! Email Id or Phone nummber is Invalid.
                    </div>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                  </div>
                <script>
                    document.addEventListener("readystatechange",function(){
                        if (document.readyState=="complete"){
                            swal("Invalid data", "Email Id or Phone nummber is Invalid.", "warning");
                        }
                    });
                    
                </script>
                <% } %>

                <button type="submit" id="submit-button">SUBMIT</button>
            </form>
        </div>
